        "huggingface_model": "t5-base"
    },
    "llama7b": {
        "llm": "llama",
        "env": {
            "LLAMA_CHECKPOINT_DIR": "/ds/models/llms/llama/7B/",
            "LLAMA_TOKENIZER_PATH": "/ds/models/llms/llama/tokenizer.model",
            "WORLD_SIZE": 1,
            "LOCAL_RANK": 0
        } 
    },
    "llama13b": {
        "llm": "llama",
        "env": {
            "LLAMA_CHECKPOINT_DIR": "/ds/models/llms/llama/13B/",
            "LLAMA_TOKENIZER_PATH": "/ds/models/llms/llama/tokenizer.model",
            "WORLD_SIZE": 1,
            "LOCAL_RANK": 0
        }
    }
