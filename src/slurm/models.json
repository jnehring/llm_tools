{
    "falcon7b": {
        "llm": "automodel",
        "huggingface_model": "tiiuae/falcon-7b",
        "env": {
            "TRUST_REMOTE_CODE": true
        }
    },
    "pythia6_9b": {
        "llm": "automodel",
        "huggingface_model": "EleutherAI/pythia-6.9b"
    },
    "pythia12b": {
        "llm": "automodel",
        "huggingface_model": "EleutherAI/pythia-12b"
    },
    "vicuna13b": {
        "llm": "vicuna",
        "env": {
            "MODEL_PATH": "/ds/models/llms/vicuna/13B",
            "LLAMA_TOKENIZER_PATH": "/ds/models/llms/llama/tokenizer.model"
        } 
    },
    "vicuna7b": {
        "llm": "vicuna",
        "env": {
            "MODEL_PATH": "/ds/models/llms/vicuna/7B",
            "LLAMA_TOKENIZER_PATH": "/ds/models/llms/llama/tokenizer.model"
        } 
    },
    "flan_t5_large": {
        "llm": "t5",
        "huggingface_model": "google/flan-t5-large"
    },
    "t5_base": {
